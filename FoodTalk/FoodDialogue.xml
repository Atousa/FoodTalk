<dialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd">
    <flow>
        <folder label="Main" id="Main">
            <output>
                <prompt selectionType="RANDOM">
                    <item>Greeting: Enter the type of food you want.</item>
                </prompt>
                <goto ref="getUserInput_2449614"/>
            </output>
            <getUserInput id="getUserInput_2449614">
                <search ref="folder_2449611"/>
                <default>
                    <output>
                        <prompt selectionType="RANDOM">
                            <item>
                                Error: I am sorry, I did not understand
                                your question. Please try asking
                                another one.
                            </item>
                        </prompt>
                    </output>
                </default>
            </getUserInput>
        </folder>
        <folder label="Library" id="Library">
            <folder label="Live Content" id="folder_2449611">
                <input>
                    <grammar>
                        <item>What type of food do you have?</item>
                        <item>$ What type of food do you have?</item>
                        <item>$ list of type</item>
                        <item>What * type * have</item>
                        <item>$ list type * available</item>
                        <item>$ what type do you have</item>
                    </grammar>
                    <output>
                        <prompt selectionType="RANDOM">
                            <item>We have Chinese, Vietnamese, American, Italian, French, Korean, Japanese, Mexican, Peruvian, British</item>
                        </prompt>
                    </output>
                </input>
            </folder>
        </folder>
        <folder label="Concepts" id="Concepts"></folder>
    </flow>
</dialog>
